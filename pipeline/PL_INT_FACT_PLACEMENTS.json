					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "WITH\nQ_DIM_COMPANY\nAS\n(SELECT COMPANY_SK,COMPANY_BK\nFROM KNE_INT.DIM_COMPANY),\n\nQ_DIM_DATES\nAS\n(SELECT REP_DATES_SK,REP_DATES_BK\n FROM KNE_INT.DIM_REPORTING_DATES\n WHERE REP_DATES_ACTIVE_IND='Y')\n\n\n\nSELECT\nQ_DIM_COMPANY.COMPANY_SK FACT_KE_PLACE_COMP_SK, \nQ_DIM_DATES.REP_DATES_SK  FACT_KE_PLACE_DATE_SK,\nFCT.PROJECT_CODE AS FACT_KE_PLACE_PROJECT,\nFCT.Partner AS FACT_KE_PLACE_PARTNER,\nFCT.University AS FACT_KE_PLACE_UNIVERSITY,\nFCT.INTERVENTION AS FACT_KE_PLACE_INTERVENTION,\nFCT.ROLE AS FACT_KE_PLACE_ROLE,\nFCT.Degree AS FACT_KE_PLACE_DEGREE,\nFCT.Year_Graduated AS FACT_KE_PLACE_GRAD_YEAR,\nFCT.No_of_Placements AS FACT_KE_PLACE_NO_OF_PLACE,\nFCT.Length_Weeks FACT_KE_PLACE_NO_OF_WEEKS,\nFCT.\"GRANT\"  AS FACT_KE_PLACE_GRANT,\nFCT.Salary AS FACT_KE_PLACE_SALARY,\nFCT.\"Match\" AS FACT_KE_PLACE_MATCH\nFROM KNE_STG.PLACEMENTS FCT\nINNER JOIN Q_DIM_COMPANY\nON FCT.BUSINESS_NUMBER=Q_DIM_COMPANY.COMPANY_BK\nINNER JOIN Q_DIM_DATES\nON CASE\nWHEN FCT.QTR = '0' THEN 'N/A'\nWHEN FCT.QTR IS NULL THEN 'N/A'\nELSE FCT.QTR\nEND=Q_DIM_DATES.REP_DATES_BK",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
