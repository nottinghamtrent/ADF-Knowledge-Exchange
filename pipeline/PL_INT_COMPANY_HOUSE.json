				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "\n\nSELECT Company_Name\n      ,Company_Number\n      ,Reg_Address_Care_Of\n      ,Reg_Address_PO_Box\n      ,Address_Line_1\n      ,Address_Line_2\n      ,Town\n      ,County\n      ,Country\n      ,Postcode\n      ,Company_Category\n      ,Company_Status\n      ,Country_of_Origin\n    ,\n\tCASE\n\tWHEN LEN(Dissolution_Date)> 0\n\tTHEN TRY_CONVERT(VARCHAR(200),Dissolution_Date,103 ) \n\tEND Dissolution_Date\n  \t,CASE\n\tWHEN LEN(Incorporation_Date) > 0\n\tTHEN TRY_CONVERT(VARCHAR(200), Incorporation_Date,103) \n\tEND Incorporation_Date     \n      ,Account_Refday\n      ,Account_Refmonth\n      ,\n\t CASE\n\t WHEN LEN(Accounts_NextDueDate) > 0\n\t THEN TRY_CONVERT(VARCHAR(20),Accounts_NextDueDate,103 )  \n\t END Accounts_NextDueDate\n      ,\n\t  CASE\n\t WHEN LEN(Accounts_LastMadeupDate) > 0\n\t THEN  TRY_CONVERT(VARCHAR(20),Accounts_LastMadeupDate,103 ) \n\t END Accounts_LastMadeupDate\n      ,Acoounts_Category\n      ,\n\t  CASE \n\t  WHEN LEN(Returns_NextDueDate)>0\n\t  THEN TRY_CONVERT(VARCHAR(20),Returns_NextDueDate,103 )  \n\t  END Returns_NextDueDate\n      ,\n\t  CASE\n\t  WHEN LEN(Returns_LastMadeUpDate) > 0\n\t  THEN TRY_CONVERT(VARCHAR(20),Returns_LastMadeUpDate,103 )  \n\t  END Returns_LastMadeUpDate\n      ,Mortgage_Charges\n      ,Mortgage_Outstanding\n      ,Mortgage_Part_Satisifed\n      ,SICCode_SicText_1\n      ,SICCode_SicText_2\n      ,SICCode_SicText_3\n      ,SICCode_SicText_4\n      ,LimitedPartnerships_NumGenPartners\n      ,LimitedPartnerships_NumLimPartners\n      ,URI AS URL\n      ,\n\t  CASE\n\t  WHEN LEN(PreviousName_1_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_1_CONDATE,103 )  \n\t  END PreviousName_1_CONDATE\n  \n       ,\n\t  CASE\n\t  WHEN LEN(PreviousName_2_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_2_CONDATE,103 )  \n\t  END PreviousName_2_CONDATE\n     \n\n      ,\n\t  CASE\n\t  WHEN LEN(PreviousName_3_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_3_CONDATE,103 )  \n\t  END PreviousName_3_CONDATE\n\n\t    ,\n\t  CASE\n\t  WHEN LEN(PreviousName_4_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_4_CONDATE,103 )  \n\t  END PreviousName_4_CONDATE\n    \n\t  ,\n\t  CASE\n\t  WHEN LEN(PreviousName_5_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_5_CONDATE,103 )  \n\t  END PreviousName_5_CONDATE\n\n\t    ,\n\t  CASE\n\t  WHEN LEN(PreviousName_6_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_6_CONDATE,103 )  \n\t  END PreviousName_6_CONDATE\n\n\n\t    ,\n\t  CASE\n\t  WHEN LEN(PreviousName_7_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_7_CONDATE,103 )  \n\t  END PreviousName_7_CONDATE\n\t\n\t    ,\n\t  CASE\n\t  WHEN LEN(PreviousName_8_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_8_CONDATE,103 )  \n\t  END PreviousName_8_CONDATE\n\n\t    ,\n\t  CASE\n\t  WHEN LEN(PreviousName_9_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_9_CONDATE,103 )  \n\t  END PreviousName_9_CONDATE\n\n\t    ,\n\t  CASE\n\t  WHEN LEN(PreviousName_10_CONDATE) >0\n\t  THEN TRY_CONVERT(VARCHAR(20),PreviousName_10_CONDATE,103 )  \n\t  END PreviousName_10_CONDATE\n\t\n\t ,PreviousName_1_CompanyName\n\t ,PreviousName_2_CompanyName\n\t ,PreviousName_3_CompanyName\n     ,PreviousName_4_CompanyName\n\t ,PreviousName_5_CompanyName\n     ,PreviousName_6_CompanyName\n     ,PreviousName_7_CompanyName\n     ,PreviousName_8_CompanyName\n     ,PreviousName_9_CompanyName\n     ,PreviousName_10_CompanyName\n\n\n\t ,CASE\n\t  WHEN LEN(ConfStmtNextDueDate) > 0\n\t  THEN TRY_CONVERT(VARCHAR(max),ConfStmtNextDueDate,103 ) \n\t  END ConfStmtNextDueDate\n      \n\t  ,CASE\n\t  WHEN LEN(ConfStmtLastMadeUpDate) >0\n\t  THEN TRY_CONVERT(VARCHAR(max),ConfStmtLastMadeUpDate,103 ) \n\t  END ConfStmtLastMadeUpDate\n  FROM KNE_STG.Company_House\n\n",
						"queryTimeout": "02:00:00",
