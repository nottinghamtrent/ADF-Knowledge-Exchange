				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "WITH\nQ_DIM_COMPANY\nAS\n(SELECT COMPANY_SK,COMPANY_BK\nFROM KNE_INT.DIM_COMPANY),\n\nQ_DIM_DATES\nAS\n(SELECT REP_DATES_SK,REP_DATES_BK\n FROM KNE_INT.DIM_REPORTING_DATES\n WHERE REP_DATES_ACTIVE_IND='Y'),\n\nQ_REPORTING_STRUCUTURE\nAS\n(SELECT BI_REP_STRUCT_SK, BI_REP_STRUCT_DEPT\n FROM KNE_INT.DIM_REPORTING_STRUCTURE\n WHERE BI_REP_STRUCT_ACTIVE_IND='Y')\n\n\n\n\n\nSELECT\nQ_DIM_COMPANY.COMPANY_SK FACT_KE_INT_COMP_SK,\nQ_DIM_DATES.REP_DATES_SK  FACT_KE_INT_DATE_SK,\nQ_REPORTING_STRUCUTURE.BI_REP_STRUCT_SK FACT_KE_INT_REP_STRUCT_SK,\nFCT.PROJECT AS FACT_KE_INT_PROJECT,\nFCT.PARTNER AS FACT_KE_INT_PARTNER,\nFCT.INTERVENTION AS FACT_KE_INT_INTERVENTION,\nFCT.INTERVENTION_TYPE AS FACT_KE_INT_INTERVENTION_TYP,\nFCT.INTERVENTION_HOURS AS FACT_KE_INT_INTERVENTION_HOURS,\nFCT.INTERVENTION_tCO2e AS FACT_KE_INT_INTERVENTION_CO2\nFROM KNE_STG.Interventions FCT\nINNER JOIN Q_DIM_COMPANY\nON FCT.BUSINESS_NUMBER=Q_DIM_COMPANY.COMPANY_BK\nINNER JOIN Q_DIM_DATES\nON CASE\nWHEN FCT.INTERVENTION_DATE = '0' THEN 'N/A'\nWHEN FCT.INTERVENTION_DATE IS NULL THEN 'N/A'\nELSE FCT.INTERVENTION_DATE\nEND=Q_DIM_DATES.REP_DATES_BK\nINNER JOIN Q_REPORTING_STRUCUTURE\nON CASE\nWHEN FCT.SCHOOL_DEBT = 'ADBE' THEN 'ENV'\nWHEN FCT.SCHOOL_DEBT = 'Art & Design' THEN 'ART'\nWHEN FCT.SCHOOL_DEBT = 'Employability' THEN 'EMP'\nWHEN FCT.SCHOOL_DEBT = 'Estates' THEN 'EST'\nWHEN FCT.SCHOOL_DEBT = 'NBS' THEN 'NBS'\nEND=Q_REPORTING_STRUCUTURE.BI_REP_STRUCT_DEPT\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE KNE_INT.FACT_INTERVENTIONS",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
